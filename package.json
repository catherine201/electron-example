{
  "name": "antd-demo-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@rematch/core": "^1.1.0",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.0",
    "@types/react": "16.8.12",
    "@types/react-dom": "16.8.3",
    "@types/react-redux": "^7.0.8",
    "@types/react-router": "^5.0.0",
    "antd": "^3.16.2",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.11.0",
    "compression-webpack-plugin": "^2.0.0",
    "customize-cra": "^0.2.12",
    "electron-localshortcut": "^3.1.0",
    "electron-updater": "^4.1.2",
    "history": "^4.9.0",
    "less": "^3.9.0",
    "ora": "^3.4.0",
    "react": "^16.8.6",
    "react-app-rewired": "^2.1.1",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux-actions": "^2.6.5",
    "redux-connect": "^9.0.0",
    "scp2": "^0.5.0",
    "ssh2": "^0.8.4",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^4.0.0",
    "typescript": "3.4.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "dist": "electron-builder",
    "electron-start": "electron .",
    "build": "react-app-rewired build",
    "deploy": "npm run build && node ./deploy",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "***.app",
    "copyright": "back-manage",
    "productName": "back-manage",
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "runAfterFinish": true,
      "installerIcon": "public/favicon1.png",
      "uninstallerIcon": "public/favicon1.png"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://***"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "public/favicon1.png",
      "artifactName": "${productName}_setup_${version}.${ext}",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "public/favicon1.png",
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^4.2.9",
    "electron-builder": "^21.2.0",
    "http-proxy-middleware": "^0.19.1",
    "less-loader": "^4.1.0"
  }
}
